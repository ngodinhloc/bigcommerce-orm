{
  "name": "bigcommerce/orm",
  "description": "Bigcommerce ORM",
  "keywords": [
    "Bigcommerce",
    "ORM",
    "Entity",
    "Entity Manager",
    "Repository"
  ],
  "homepage": "https://github.com/bigcommerce/",
  "license": "MIT",
  "authors": [
    {
      "name": "Ken Ngo",
      "email": "ken.ngo@bigcommerce.com.au"
    }
  ],

  "require": {
    "php": "^7.3||^8.0",
    "doctrine/annotations": "^1.6",
    "psr/log": "^1.1",
    "psr/http-message": "^1.0",
    "psr/cache": "^1.0",
    "guzzlehttp/guzzle": "^6.4.1",
    "symfony/event-dispatcher": "^2.8",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "phpspec/prophecy": "^1.0",
    "monolog/monolog": "^2.1.1",
    "guzzlehttp/streams": "^3.0@dev"
  },
  "autoload": {
    "psr-4": {
      "Bigcommerce\\ORM\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "Samples\\": "samples/"
    }
  },
  "minimum-stability": "dev"
}
